<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alex.antdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:name="com.alex.antdemo.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.alex.antdemo.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.alex.antdemo.activities.DisplayActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.alex.antdemo.activities.DevListActivity"
            android:label="@string/app_name" >
        </activity>
        
        <service android:name="com.alex.antdemo.LocalService">
            
		</service> 
    </application>

</manifest>
